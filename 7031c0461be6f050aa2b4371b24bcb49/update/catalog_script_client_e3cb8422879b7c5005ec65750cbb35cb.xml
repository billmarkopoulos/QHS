        <script><![CDATA[function onSubmit() {
      var ga = new GlideAjax('QHSrevisionFromCatalog');

      ga.addParam('sysparm_name', 'updateRecord');

      ga.addParam('sysparm_recordid', g_form.getValue("name"));
	
	ga.addParam('sysparm_userid', g_user.userID);

      ga.getXML(updateCampus);
	
	
	return false;

}

     

function updateCampus(response) {
	
	var answer = response.responseXML.documentElement.getAttribute("answer");
	
	if (answer == 'in_revision'){
		g_form.addErrorMessage('Το αρχείο βρίσκεται ήδη υπό αναθεώρηση.');
	}
	else{
		top.window.location = answer;
	}
	
	return false;
   
}]]></script>
