	if ((parseFloat(dif.getDayPart()) - parseFloat(current.duration_edit))>7){
		current.overdue_commentary = true;
	}
	
	current.duration_commentary = parseFloat(dif.getDayPart()) - parseFloat(current.duration_edit);

})(current, previous);]]></script>
