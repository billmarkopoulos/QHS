        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	
	var gr = new GlideRecord('x_epats_qhs_qhs_policy');
	gr.addQuery('sys_id', current.x_epats_qhs_qhs_pointer.sys_id);
	gr.query();
	
	if (gr.next()){
		if (current.x_epats_qhs_qhs_is_revisal_program==false){
			gr.u_journal_2 = 'Αναθεώρηση εγγράφου';
			gr.setValue('assigned_to', gr.getValue('qhs_manager'));
			gr.setValue('procedure_2','update');
			gr.setValue('state','draft');
		}
		else{
			gr.setValue('assigned_to', gr.getValue('qhs_director'));
			gr.setValue('state','approval');
		}
		gr.update();
	}
	
	if (current.x_epats_qhs_qhs_is_revisal_program==true){
		current.workflow_state = 'retired';
	}
	
	current.x_epats_qhs_qhs_revise_check = false;

})(current, previous);]]></script>
