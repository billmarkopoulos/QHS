        <name>QHSRetireButton</name>
        <script><![CDATA[var QHSRetireButton = Class.create();
QHSRetireButton.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {

    retireArticle: function () {
		
		var articleid = this.getParameter('sysparm_articleid');
		
		var gr = new GlideRecord('kb_knowledge');
		gr.addQuery('sys_id',articleid);
		gr.query();
		
		if (gr.next()){
			gr.setValue('workflow_state','retired');
			gr.update();
		}
		

      }
});]]></script>
