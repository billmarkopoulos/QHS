        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	
	var user_ids = current.qhs_manager + ',' + current.qhs_director + ',' + current.armodios_manager + ',' + current.u_glide_list_9 + ',' + current.u_glide_list_10;
	
	if (current.revise_send_to_general_director){
		user_ids = user_ids + current.general_director;
	}
	
	var final_wl = '';
	
	if (user_ids.length>1){
		var wl = user_ids.split(',');
		var added = [];
		for (var i=0;i<wl.length;i++){
			var bl=0;
			for (var j=0;j<added.length;j++){
				if (wl[i]==added[i]){
					bl=1;
				}
			}
			if (bl==0){
				final_wl = final_wl + ',' + wl[i];
				added.push(wl[i]);
			}
		}
	}
	
	current.u_glide_list_1 = final_wl;

})(current, previous);]]></script>
